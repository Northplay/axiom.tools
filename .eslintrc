{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
        "document": true,
        "window": true,
        "process": true
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/jsx-wrap-multilines": 0,
        "react/jsx-closing-tag-location": 0,
        "react/no-typos": 0,
        "react/no-danger": 0,
        "object-curly-newline": [
            "error", {
                "ObjectPattern": "never"
            }
        ]
    }
}
